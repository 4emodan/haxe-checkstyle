{
	"checks": [
		{
			"type": "Anonymous",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "ArrayLiteral",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "AvoidInlineConditionals",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "AvoidStarImport",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "ConstantName",
			"props": {
				"severity": "IGNORE",
				"format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
				"tokens": ["INLINE", "NOTINLINE"]
			}
		},
		{
			"type": "CyclomaticComplexity",
			"props": {
				"severity": "IGNORE",
				"thresholds": [
					{
						"severity": "WARNING",
						"complexity": 20
					},
					{
						"severity": "ERROR",
						"complexity": 25
					}
				]
			}
		},
		{
			"type": "Dynamic",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "EmptyBlock",
			"props": {
				"severity": "IGNORE",
				"option": "empty",
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH",
					"OBJECT_DECL",
					"FUNCTION"
				]
			}
		},
		{
			"type": "EmptyLines",
			"props": {
				"severity": "IGNORE",
				"max": 1,
				"allowEmptyLineAfterSingleLineComment": true,
				"allowEmptyLineAfterMultiLineComment": true
			}
		},
		{
			"type": "ERegLiteral",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "FileLength",
			"props": {
				"severity": "IGNORE",
				"max": 2000
			}
		},
		{
			"type": "HexadecimalLiteral",
			"props": {
				"severity": "IGNORE",
				"option": "upperCase"
			}
		},
		{
			"type": "HiddenField",
			"props": {
				"severity": "IGNORE",
				"ignoreConstructorParameter": true,
				"ignoreSetter": true,
				"ignoreFormat": "^(main|run)$"
			}
		},
		{
			"type": "IndentationCharacter",
			"props": {
				"severity": "IGNORE",
				"character": "tab"
			}
		},
		{
			"type": "InnerAssignment",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "LeftCurly",
			"props": {
				"severity": "IGNORE",
				"option": "eol",
				"ignoreEmptySingleline": true,
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"CLASS_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				]
			}
		},
		{
			"type": "LineLength",
			"props": {
				"severity": "IGNORE",
				"max": 160
			}
		},
		{
			"type": "ListenerName",
			"props": {
				"severity": "IGNORE",
				"format": "^_?[a-z][a-zA-Z0-9]*$",
				"listeners": [
					"addEventListener",
					"addListener",
					"on",
					"once"
				]
			}
		},
		{
			"type": "LocalVariableName",
			"props": {
				"severity": "IGNORE",
				"format": "^[a-z][a-zA-Z0-9]*$"
			}
		},
		{
			"type": "MagicNumber",
			"props": {
				"severity": "IGNORE",
				"ignoreNumbers": [-1, 0, 1, 2]
			}
		},
		{
			"type": "MemberName",
			"props": {
				"severity": "IGNORE",
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": [
					"CLASS",
					"ABSTRACT",
					"PUBLIC",
					"PRIVATE",
					"TYPEDEF",
					"ENUM"
				]
			}
		},
		{
			"type": "MethodLength",
			"props": {
				"severity": "IGNORE",
				"max": 50
			}
		},
		{
			"type": "MethodName",
			"props": {
				"severity": "IGNORE",
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": [
					"PUBLIC",
					"PRIVATE",
					"STATIC",
					"NOTSTATIC",
					"INLINE",
					"NOTINLINE"
				]
			}
		},
		{
			"type": "ModifierOrder",
			"props": {
				"severity": "IGNORE",
				"modifiers": [
					"MACRO",
					"OVERRIDE",
					"PUBLIC_PRIVATE",
					"STATIC",
					"INLINE",
					"DYNAMIC"
				]
			}
		},
		{
			"type": "MultipleStringLiterals",
			"props": {
				"severity": "IGNORE",
				"allowDuplicates": 2,
				"minLength": 2,
				"ignore": "^\\s+$"
			}
		},
		{
			"type": "MultipleVariableDeclarations",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "NeedBraces",
			"props": {
				"severity": "IGNORE",
				"allowSingleLineStatement": true,
				"tokens": [
					"IF",
					"ELSE_IF",
					"FOR",
					"WHILE",
					"DO_WHILE"
				]
			}
		},
		{
			"type": "NestedForDepth",
			"props": {
				"severity": "IGNORE",
				"max": 1
			}
		},
		{
			"type": "NestedIfDepth",
			"props": {
				"severity": "IGNORE",
				"max": 1
			}
		},
		{
			"type": "NestedTryDepth",
			"props": {
				"severity": "IGNORE",
				"max": 1
			}
		},
		{
			"type": "OperatorWrap",
			"props": {
				"severity": "IGNORE",
				"option": "eol",
				"tokens": [
						  "=", "+", "-", "*", "/", "%", ">", "<",
						  ">=", "<=", "==", "!=", "&", "|", "^", "&&",
						  "||", "<<", ">>", ">>>", "+=", "-=", "*=", "/=",
						  "%=", "<<=", ">>=", ">>>=", "|=", "&=", "^=", "...",
						  "=>", "++", "--"
				]
			}
		},
		{
			"type": "OperatorWrap",
			"props": {
				"severity": "IGNORE",
				"option": "nl",
				"tokens": [
							"!"
				]
			}
		},
		{
			"type": "ParameterName",
			"props": {
				"severity": "IGNORE",
				"format": "(_|^[a-z][a-zA-Z0-9]*)$"
			}
		},
		{
			"type": "ParameterNumber",
			"props": {
				"severity": "IGNORE",
				"max": 7,
				"ignoreOverriddenMethods": false
			}
		},
		{
			"type": "RedundantModifier",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "Return",
			"props": {
				"severity": "IGNORE",
				"allowEmptyReturn": true
			}
		},
		{
			"type": "RightCurly",
			"props": {
				"severity": "IGNORE",
				"option": "aloneorsingle",
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"CLASS_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				]
			}
		},
		{
			"type": "SeparatorWrap",
			"props": {
				"severity": "IGNORE",
				"option": "eol",
				"tokens": [
							","
				]
			}
		},
		{
			"type": "SeparatorWrap",
			"props": {
				"severity": "IGNORE",
				"option": "nl",
				"tokens": [
							"."
				]
			}
        },
		{
			"type": "SimplifyBooleanExpression",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "Spacing",
			"props": {
				"severity": "INFO",
				"spaceAroundBinop": true,
				"noSpaceAroundUnop": true,
				"spaceIfCondition": true,
				"spaceIfCondition": true,
				"spaceForLoop": true,
				"spaceWhileLoop": true,
				"spaceSwitchCase": true,
				"spaceCatch": true,
				"ignoreRangeOperator": true
			}
		},
		{
			"type": "TabForAligning",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "TODOComment",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "TrailingWhitespace",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "Type",
			"props": {
				"severity": "IGNORE",
				"ignoreEnumAbstractValues": true
			}
		},
		{
			"type": "TypeName",
			"props": {
				"severity": "IGNORE",
				"format": "^I[A-Z][a-zA-Z0-9]*$",
				"tokens": [
					"INTERFACE"
				]
			}
		},
		{
			"type": "TypeName",
			"props": {
				"severity": "IGNORE",
				"format": "^[A-Z][a-zA-Z0-9]*$",
				"tokens": [
					"CLASS",
					"ENUM",
					"TYPEDEF"
				]
			}
		},
		{
			"type": "VariableInitialisation",
			"props": {
				"severity": "IGNORE"
			}
		},
		{
			"type": "WhitespaceAfter",
			"props": {
				"severity": "IGNORE",
				"tokens": [ ",", ";" ]
			}
		},
		{
			"type": "WhitespaceAround",
			"props": {
				"severity": "IGNORE",
				"tokens": [ 
					"=", "+", "-", "*", "/", "%", ">", "<",
					">=", "<=", "==", "!=", "&", "|", "^", "&&",
					"||", "<<", ">>", ">>>", "+=", "-=", "*=", "/=",
					"%=", "<<=", ">>=", ">>>=", "|=", "&=", "^="
				]
			}
		}
	]
}